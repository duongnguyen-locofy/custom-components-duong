{
  "name": "rupertong-ui-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for rupertong-ui library",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run bootstrap && npm run build || exit 1",
    "create-package": "node src/scripts/createPackage.js",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 lerna run build",
    "test": "cross-env TZ=America/New_York jest --coverage --silent --maxWorkers=50%",
    "test:ci": "jest --coverage --silent --runInBand",
    "test:debug": "jest --runInBand --logHeapUsage --detectOpenHandles --debug",
    "test:watch": "jest --watch",
    "storybook": "cross-env start-storybook -p 6006 --no-manager-cache --quiet",
    "build-storybook": "build-storybook --quiet",
    "lint": "eslint \"./packages/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"./packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "bootstrap": "lerna bootstrap --hoist",
    "prerelease:ci": "lerna version --yes --conventional-commits --conventional-graduate --no-commit-hooks --no-private",
    "release:ci": "lerna publish from-package --yes --no-commit-hooks",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "@https://github.com/rupert-ong/rupertong-ui"
  },
  "publishConfig": {
    "scope": "@rupertong-ui",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/addon-storysource": "^6.3.0",
    "@storybook/addons": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@storybook/theming": "^6.3.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^3.8.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "lerna-update-wizard": "^1.1.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "storybook-readme": "^5.0.9",
    "ts-jest": "^26.5.5",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "packages/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
